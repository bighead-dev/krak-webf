{
    "name": "krak/webf",
    "description": "Symfony Based WebFramework",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/bighead-dev/krak-webf",
    "keywords" : [
        "webframework",
        "symfony"
    ],
    "authors" : [
        {
            "name" : "RJ Garcia",
            "email" : "rj@bighead.net"
        }
    ],
    "require" : {
        "symfony/routing" : "2.*",
        "symfony/http-foundation" : "2.*",
        "symfony/http-kernel" : "2.*"
    },
    "require-dev" : {
        "phpunit/phpunit" : "4.0.*",
        "symfony/browser-kit" : "2.5.*"
    },
    "autoload" : {
        "psr-4" : {
            "Krak\\Webf\\" : "src"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Krak\\Tests\\" : "tests"
        }
    },
    "minimum-stability" : "dev"
}
